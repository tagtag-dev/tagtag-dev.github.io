6,160,163],"ul",{},[157,158,159],"li",{},"用户 (User) 与角色 (Role) 多对多关联",[157,161,162],{},"角色 (Role) 与权限 (Permission) 多对多关联",[157,164,165],{},"权限 (Permission) 与资源 (Resource) 一一对应",[140,167,169],{"id":168},"_12-权限类型","1.2 权限类型",[113,171,172],{},"Tagtag Starter 的 RBAC 模型支持两种类型的权限：",[174,175,177],"h4",{"id":176},"_121-功能权限","1.2.1 功能权限",[113,179,180],{},"控制用户是否可以访问系统的某个功能或操作，包括：",[154,182,183,186,189],{},[157,184,185],{},"菜单访问权限",[157,187,188],{},"按钮操作权限",[157,190,191],{},"API 接口权限",[113,193,194,195,199],{},"功能权限的标识格式为：",[196,197,198],"code",{},"{模块}:{资源}:{操作}","，例如：",[154,201,202,208,214,220],{},[157,203,204,207],{},[196,205,206],{},"sys:user:view"," - 查看用户",[157,209,210,213],{},[196,211,212],{},"sys:user:add"," - 新增用户",[157,215,216,219],{},[196,217,218],{},"sys:user:update"," - 更新用户",[157,221,222,225],{},[196,223,224],{},"sys:user:delete"," - 删除用户",[174,227,229],{"id":228},"_122-数据权限","1.2.2 数据权限",[113,231,232],{},"控制用户可以访问的数据范围，包括：",[154,234,235,238,241,244],{},[157,236,237],{},"全部数据：可以访问所有数据",[157,239,240],{},"本部门数据：只能访问所属部门的数据",[157,242,243],{},"本部门及子部门数据：可以访问所属部门及其子部门的数据",[157,245,246],{},"自定义数据：根据自定义规则过滤数据",[140,248,250],{"id":249},"_13-权限继承","1.3 权限继承",[113,252,253],{},"权限支持继承关系：",[154,255,256,259],{},[157,257,258],{},"目录权限继承：如果用户拥有某个目录的访问权限，默认拥有该目录下所有子菜单的访问权限",[157,260,261],{},"角色权限继承：可以创建角色层级，子角色继承父角色的权限",[121,263,265],{"id":264},"_2-数据模型","2. 数据模型",[140,267,269],{"id":268},"_21-用户-user","2.1 用户 (User)",[113,271,272],{},"代表系统的操作员。",[113,274,275,278],{},[116,276,277],{},"属性",":",[154,280,281,284,287,290,293,296,299,302,305],{},[157,282,283],{},"用户名 (username)",[157,285,286],{},"密码 (password，BCrypt加密)",[157,288,289],{},"姓名 (name)",[157,291,292],{},"邮箱 (email)",[157,294,295],{},"电话 (phone)",[157,297,298],{},"状态 (status: 1-启用, 0-禁用)",[157,300,301],{},"归属部门 (dept_id)",[157,303,304],{},"创建时间 (create_time)",[157,306,307],{},"更新时间 (update_time)",[113,309,310,278],{},[116,311,312],{},"关联",[154,314,315,318],{},[157,316,317],{},"一个用户可以拥有多个角色 (多对多关系)",[157,319,320],{},"一个用户属于一个部门 (多对一关系)",[140,322,324],{"id":323},"_22-角色-role","2.2 角色 (Role)",[113,326,327],{},"代表一组权限的集合。",[113,329,330,278],{},[116,331,277],{},[154,333,334,337,340,342,345],{},[157,335,336],{},"角色名称 (name)",[157,338,339],{},"角色编码 (code)",[157,341,298],{},[157,343,344],{},"数据权限类型 (data_scope_type)",[157,346,347],{},"描述 (description)",[113,349,350,278],{},[116,351,352],{},"功能",[154,354,355,358,361],{},[157,356,357],{},"用于批量分配权限给用户",[157,359,360],{},"可以配置角色的数据访问范围",[157,362,363],{},"支持角色层级，实现权限继承",[140,365,367],{"id":366},"_23-权限菜单-permissionmenu","2.3 权限/菜单 (Permission/Menu)",[113,369,370],{},"代表系统中的功能资源。",[113,372,373,278],{},[116,374,277],{},[154,376,377,380,383,386,389,392,395,398,401],{},[157,378,379],{},"权限名称 (name)",[157,381,382],{},"权限编码 (code)",[157,384,385],{},"权限类型 (type: 1-目录, 2-菜单, 3-按钮)",[157,387,388],{},"父级ID (parent_id)",[157,390,391],{},"路由路径 (path)",[157,393,394],{},"组件路径 (component)",[157,396,397],{},"图标 (icon)",[157,399,400],{},"排序 (sort)",[157,402,298],{},[113,404,405,278],{},[116,406,407],{},"类型",[154,409,410,416,422],{},[157,411,412,415],{},[116,413,414],{},"目录 (Directory)",": 左侧菜单栏的分组，不对应具体页面",[157,417,418,421],{},[116,419,420],{},"菜单 (Menu)",": 具体的页面入口，对应一个路由",[157,423,424,427],{},[116,425,426],{},"按钮 (Button)",": 页面内的功能操作，对应一个权限标识",[113,429,430,433,434,436],{},[116,431,432],{},"标识",": 如 ",[196,435,212],{},"，用于后端接口权限校验和前端按钮显示控制。",[140,438,440],{"id":439},"_24-部门-dept","2.4 部门 (Dept)",[113,442,443],{},"代表企业的组织层级结构。",[113,445,446,278],{},[116,447,277],{},[154,449,450,453,456,458,460],{},[157,451,452],{},"部门名称 (name)",[157,454,455],{},"部门编码 (code)",[157,457,388],{},[157,459,400],{},[157,461,298],{},[113,463,464,467],{},[116,465,466],{},"结构",": 树形结构，支持无限层级。",[113,469,470,473],{},[116,471,472],{},"用途",": 用于构建组织架构树，以及作为数据权限的判定依据。",[140,475,477],{"id":476},"_25-关系表","2.5 关系表",[154,479,480,486],{},[157,481,482,485],{},[116,483,484],{},"user_role",": 用户与角色的多对多关系表",[157,487,488,491],{},[116,489,490],{},"role_permission",": 角色与权限的多对多关系表",[121,493,495],{"id":4